{
    "name": "@helsingborg-stad/modularity",
    "version": "6.88.1",
    "description": "Modular component system for WordPress",
    "browserslist": [
        "> 1%",
        "not dead"
    ],
    "files": [
        "dist/"
    ],
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "autoprefixer": "^10.4.20",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "postcss": "^8.5.1",
        "rollup-plugin-copy": "^3.5.0",
        "sass": "^1.90.0",
        "ts-jest": "^29.4.1",
        "typescript": "^5.9.2",
        "vite": "^7.1.2",
        "vite-plugin-simple-manifest": "^1.0.2"
    },
    "scripts": {
        "test": "jest --coverage --maxWorkers=$(node -e \"console.log(Math.max(1, Math.floor(require('os').cpus().length / 2)))\")",
        "dev": "vite",
        "build": "vite build",
        "build:dev": "vite build --mode development",
        "watch": "vite build --watch --mode development"
    },
    "author": "Kristoffer Svanmark",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/helsingborg-stad/modularity.git"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|ts)$": "ts-jest"
        },
        "collectCoverageFrom": [
            "source/**/*.js",
            "source/**/*.ts"
        ],
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "testMatch": [
            "**/*.test.(js|ts)"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!(.*\\.mjs$))"
        ]
    },
    "dependencies": {
        "@helsingborg-stad/openstreetmap": "^0.51.4",
        "vite-config-factory": "^1.0.1"
    }
}
